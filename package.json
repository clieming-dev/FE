{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "check-types": "turbo run check-types",
    "mobile:dev": "turbo run dev --filter=mobile",
    "mobile:build": "pnpm --filter=mobile eas build",
    "mobile:preview": "pnpm --filter=mobile eas build --profile preview",
    "mobile:deploy": "pnpm --filter=mobile eas build --profile production --auto-submit"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
